{
  "name": "soda-js-sdk",
  "version": "1.0.2",
  "description": "This SDK provides functionalities for AES and RSA encryption schemes, ECDSA signature scheme and some functionalities used for working with sodalabs blockchain.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "js/soda-sdk.d.ts",
  "scripts": {
    "build:cjs": "webpack --config webpack.cjs.config.cjs",
    "build:esm": "webpack --config webpack.esm.config.cjs",
    "build": "npm run build:cjs && npm run build:esm",
    "test": "mocha --require esm js/test.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlehSoda/test-soda-sdk.git"
  },
  "type": "module",
  "dependencies": {
    "ethers": "^6.13.4",
    "node-forge": "^1.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/preset-env": "^7.25.8",
    "babel-loader": "^9.2.1",
    "chai": "^5.0.3",
    "ethereumjs-util": "^7.1.5",
    "esm": "^3.2.25",
    "mocha": "^10.2.0",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "license": "MIT",
  "keywords": [
    "sodalabs",
    "sdk",
    "aes",
    "rsa",
    "ecdsa",
    "encryption",
    "blockchain"
  ]
}
